nums = [3, 8, 12, 17, 15]
target = 20

def all_two_sum(nums, target):
    result = []                                      
    for i in range(len(nums)):                         
        for j in range(i + 1, len(nums)):         
            if nums[i] + nums[j] == target:     
                result.append([i, j])           
    return result                             

   

arr = all_two_sum(nums, target)
print(arr)
